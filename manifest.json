{
    "manifest_version": 3,
    "name": "Cursor Copilot",
    "author": "Sam DiSorbo",
    "version": "1.0",
    "description": "Displays a static message near the cursor.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content.js"]
      }
    ]
  }
  